<Window x:Class="SmokeTest.ReleaseReportSections"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmokeTest"
        mc:Ignorable="d"
        Title="Release Report Sections" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgSections" Margin="10,99,9.6,10" ItemsSource="{Binding Sections}"/>

    </Grid>
</Window>
